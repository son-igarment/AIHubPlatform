                                                                 "nullable":  true
                                                             },
                                             "assignee_id":  {
                                                                 "type":  "integer",
                                                                 "format":  "int32",
                                                                 "nullable":  true
                                                             },
                                             "collaborators":  {
                                                                   "type":  "array",
                                                                   "items":  {
                                                                                 "type":  "integer",
                                                                                 "format":  "int32"
                                                                             },
                                                                   "nullable":  true
                                                               },
                                             "expected_output":  {
                                                                     "type":  "string",
