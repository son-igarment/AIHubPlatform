                                                                 },
                                             "deadline":  {
                                                              "type":  "string",
                                                              "format":  "date-time",
                                                              "nullable":  true
                                                          },
                                             "status":  {
                                                            "type":  "string",
                                                            "nullable":  true
                                                        },
                                             "progress_percentage":  {
                                                                         "type":  "integer",
                                                                         "format":  "int32",
                                                                         "nullable":  true
                                                                     },
                                             "notion_link":  {
                                                                 "type":  "string",
                                                                 "nullable":  true
                                                             }
                                         },
                          "additionalProperties":  false
                      }
}
